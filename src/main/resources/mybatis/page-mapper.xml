<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tvelykyy.indexator.dao.PageDao">

    <insert id="insertPage" parameterType="com.tvelykyy.indexator.model.Page" useGeneratedKeys="true" keyProperty="id">
        insert into page(url, title)
        values( #{url}, #{title} )
    </insert>

    <insert id="insertUnindexedPage" parameterType="com.tvelykyy.indexator.model.UnindexedPage">
        insert into unindexed_page(page_id, unindexed_in_days)
        values( #{id}, #{unindexedInDays} )
    </insert>

</mapper>